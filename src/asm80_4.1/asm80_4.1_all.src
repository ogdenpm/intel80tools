    aExternalSymbol(*) byte initial(CR, LF, 'EXTERNAL SYMBOLS', CR, LF, 0),
    pad754E word,

    aUserSymbols(*) byte data(CR, LF, 'USER SYMBOLS', CR, LF, 0),
