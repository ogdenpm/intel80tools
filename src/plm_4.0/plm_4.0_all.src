$list

Sub$4889: procedure public;
	call WriteRec(.rec6, 3);
	call WriteRec(.rec22, 1);
	call WriteRec(.rec24$1, 2);
	call WriteRec(.rec24$2, 2);
	call WriteRec(.rec24$3, 2);
	call WriteRec(.rec20, 1);
