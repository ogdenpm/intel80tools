shared/clrflg.plm
ClrFlag: DO;

$include(:f3:clrflg.ipx)

ClrFlag: PROCEDURE(base, flag) PUBLIC;
	DECLARE base ADDRESS, flag BYTE;
	DECLARE b BASED base(1) BYTE;
	DECLARE off BYTE, mask BYTE;

	off = tblOffsets(flag);
	mask = not tblBitFlags(flag);
	b(off) = b(off) and mask;
end;

