fwrite:	xchg
	shld	len
	mov	h, b
	mov	l, c
	shld	buf
	pop	h
	xthl
	shld	file
	xchg
	lxi	h, bsize
	dad	d
	push	h
	xchg
	lxi	b, curoff
	dad	b
	push	h
	call	@P0099		; hl = m[de] - (bc = m[hl])
