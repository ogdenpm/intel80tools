NewPgl: procedure public;
	declare (i, j, k, m) byte,
		pnum(3) byte;

	if not PAGING then
		return;
	call wrcLst(0ch);
	linlft = PAGELEN;
	pageNo = pageNo + 1;
	i = Num2Asc(pageNo, 3, 10, .pnum);
	j = PWIDTH - 41;
	if j < TITLELEN then
		m = j;
	else
		m = TITLELEN;
	call WrnstrLst(.Plm80Compiler, 20);
	call WrnstrLst(.TITLE, m);
	k = j - m + 2;
	do while k <> 0;
		call wrcLst(' ');
		k = k - 1;
	end;
	call WrnstrLst(.DATE, 9);
	call WrnstrLst(.('  PAGE '), 7);
	call WrnstrLst(.pnum, 3);
	call WrnstrLst(.(cr, lf, lf, lf), 4);
	b3CFF = 0;
end;


NlLead: procedure public;
	call Wr2cLst(0d0ah);
	col = 0;
	linlft = linlft - 1;
	if linlft = 0 then
		call NewPgl;
	do while col < b3CFB;
		call wrcLst(' ');
		col = col + 1;
	end;
	if b3CFC <> 0 then
	do;
		call wrcLst(b3CFC);
		col = col + 1;
	end;
	do while col < b3CFD;
		call wrcLst(' ');
		col = col + 1;
	end;
end;
end;
