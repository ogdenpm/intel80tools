Sub$5F4B: procedure(arg1w, arg2w, arg3b, arg4b) public;
	declare arg1w address, arg2w address, arg3b byte, arg4b byte;
	tx2op2(tx2qp) = arg1w;
	tx2op1(tx2qp) = arg2w;
	tx2Aux1b(tx2qp) = arg3b;
	tx2Aux2b(tx2qp) = arg4b;
	if arg4b = 8 then
		tx2opc(tx2qp) = T2$NUMBER;
	else
		tx2opc(tx2qp) = T2$VARIABLE;
