L1:	lhld	len
	call	@P0095
	lhld	len
	jnc	L2
	xchg

L2:	shld	wcnt
	lhld	file
	lxi	d, bufp
	dad	d
	pop	d		; current offset
	push	d		; current offset
	call	@P0012
	xchg
	lhld	buf		; copy first lot of bytes
	mov	b, h
	mov	c, l
	lhld	wcnt
	push	h		; wcnt
	call	movmem
	pop	h		; advance the offset
	push	h		; current offset
	lxi	d, wcnt
	call	@P0012
	xchg
	pop	h
	push	h		; current offset
	mov	m, e
	inx	h
