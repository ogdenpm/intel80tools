Sub$61E0: procedure(arg1b) public;
	declare arg1b byte;
	if (b5124(tx2opc(arg1b)) and 0c0h) = 0 then
	do;
		wC1DC(bC1DB) = 0ah;
		wC1DC(bC1DB + 1) = tx2op3(arg1b);
		wC1DC(bC1DB + 2) = shl(wC1C3 - tx2op3(arg1b), 1);
		bC1DB = bC1DB + 3;
	end;
	else if tx2op1(arg1b) <> 0 then
	do;
